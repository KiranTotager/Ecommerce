  "components": {
    "schemas": {
      "AddCategoryDto": {
        "required": [
          "categoryUrl",
          "description",
          "displayHome",
          "displayOrder",
          "metaDesc",
          "name",
          "title"
        ],
        "type": "object",
        "properties": {
          "name": {
            "minLength": 1,
            "type": "string",
            "description": "provide the name of the category here"
          },
          "displayHome": {
            "type": "boolean",
            "description": "do you want to display it on home screen"
          },
          "categoryUrl": {
            "minLength": 1,
            "type": "string",
            "description": "provide the category url here"
          },
          "title": {
            "minLength": 1,
            "type": "string",
            "description": "provide the title for the category"
          },
          "description": {
            "minLength": 1,
            "type": "string",
            "description": "provide the description for the category"
          },
          "metaDesc": {
            "minLength": 1,
            "type": "string",
            "description": "provide the meta description for the category"
          },
          "displayOrder": {
            "type": "integer",
            "description": "provide the display order details here",
            "format": "int32"
          }
        },
        "additionalProperties": false
      },
      "AddCoupenDetailsDTO": {
        "required": [
          "coupenCode",
          "coupenStaus",
          "coupenType",
          "coupenValue",
          "expiryDate"
        ],
        "type": "object",
        "properties": {
          "coupenType": {
            "minLength": 1,
            "type": "string",
            "description": "please provide the type of coupen here"
          },
          "coupenCode": {
            "minLength": 1,
            "type": "string",
            "description": "this property is for the coupen code "
          },
          "coupenValue": {
            "minLength": 1,
            "type": "string",
            "description": "this property is for the coupen value "
          },
          "coupenStaus": {
            "$ref": "#/components/schemas/CoupenStatus"
          },
          "expiryDate": {
            "type": "string",
            "description": "this property is for the coupen Expiry date ",
            "format": "date-time"
          }
        },
        "additionalProperties": false
      },
      "CMSCoupenResponseDto": {
        "type": "object",
        "properties": {
          "coupenType": {
            "type": "string",
            "nullable": true
          },
          "coupenCode": {
            "type": "string",
            "nullable": true
          },
          "coupenValue": {
            "type": "string",
            "nullable": true
          },
          "coupenStatus": {
            "$ref": "#/components/schemas/CoupenStatus"
          },
          "coupenExpiryDate": {
            "type": "string",
            "format": "date-time"
          }
        },
        "additionalProperties": false
      },
      "CategoryResponseDto": {
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "format": "int32"
          },
          "categoryName": {
            "type": "string",
            "nullable": true
          },
          "isDisplayHome": {
            "type": "boolean"
          },
          "catImageUrl": {
            "type": "string",
            "nullable": true
          },
          "catUrl": {
            "type": "string",
            "nullable": true
          },
          "catPageTitle": {
            "type": "string",
            "nullable": true
          },
          "catDescription": {
            "type": "string",
            "nullable": true
          },
          "catMetaDescription": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "CoupenStatus": {
        "enum": [
          "Active",
          "Expired",
          "Used",
          "Inactive"
        ],
        "type": "string"
      },
      "ObjectResponseBase": {
        "type": "object",
        "properties": {
          "statusCode": {
            "type": "integer",
            "format": "int32"
          },
          "message": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "ProblemDetails": {
        "type": "object",
        "properties": {
          "type": {
            "type": "string",
            "nullable": true
          },
          "title": {
            "type": "string",
            "nullable": true
          },
          "status": {
            "type": "integer",
            "format": "int32",
            "nullable": true
          },
          "detail": {
            "type": "string",
            "nullable": true
          },
          "instance": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": { }
      },
      "ProductResponseDto": {
        "type": "object",
        "properties": {
          "productId": {
            "type": "string",
            "format": "uuid"
          },
          "productName": {
            "type": "string",
            "nullable": true
          },
          "productCreatedAt": {
            "type": "string",
            "format": "date-time"
          },
          "productUpdatedAt": {
            "type": "string",
            "format": "date-time"
          },
          "productImageUrl": {
            "type": "string",
            "nullable": true
          },
          "productAddedIp": {
            "type": "string",
            "nullable": true
          },
          "productCategoryName": {
            "type": "string",
            "nullable": true
          },
          "productDescription": {
            "type": "string",
            "nullable": true
          },
          "productPrice": {
            "type": "number",
            "format": "double"
          },
          "productDiscount": {
            "type": "number",
            "format": "double"
          },
          "productStockQuantity": {
            "type": "integer",
            "format": "int32"
          },
          "productAddedBy": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      }
    },
    "securitySchemes": {
      "Bearer": {
        "type": "http",
        "description": "jwt authorization using the bearer scheme",
        "scheme": "Bearer"
      },
      "GuestId": {
        "type": "apiKey",
        "description": "Optional guest id for the anonymous user",
        "name": "GuestId",
        "in": "header"
      }
    }
  },
