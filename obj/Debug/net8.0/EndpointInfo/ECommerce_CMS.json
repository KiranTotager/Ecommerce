    "/api/v1/Product/product/add": {
      "post": {
        "tags": [
          "Product"
        ],
        "summary": "use this end call to add new procuct",
        "requestBody": {
          "content": {
            "multipart/form-data": {
              "schema": {
                "required": [
                  "ProductCategory",
                  "ProductDescription",
                  "ProductImage",
                  "ProductName",
                  "ProductPrice",
                  "StockQuantity"
                ],
                "type": "object",
                "properties": {
                  "ProductName": {
                    "type": "string",
                    "description": "provide the name of the product"
                  },
                  "ProductDescription": {
                    "type": "string",
                    "description": "provide the descritpion for the product"
                  },
                  "ProductPrice": {
                    "type": "number",
                    "description": "provide the price for the product here",
                    "format": "double"
                  },
                  "ProductDiscount": {
                    "type": "number",
                    "description": "provide the description for the product",
                    "format": "double"
                  },
                  "StockQuantity": {
                    "type": "integer",
                    "description": "provide the available quantity of product",
                    "format": "int32"
                  },
                  "ProductImage": {
                    "type": "string",
                    "description": "provide the image of the product",
                    "format": "binary"
                  },
                  "ProductCategory": {
                    "type": "string",
                    "description": "provide the product category name"
                  }
                }
              },
              "encoding": {
                "ProductName": {
                  "style": "form"
                },
                "ProductDescription": {
                  "style": "form"
                },
                "ProductPrice": {
                  "style": "form"
                },
                "ProductDiscount": {
                  "style": "form"
                },
                "StockQuantity": {
                  "style": "form"
                },
                "ProductImage": {
                  "style": "form"
                },
                "ProductCategory": {
                  "style": "form"
                }
              }
            }
          }
        },
